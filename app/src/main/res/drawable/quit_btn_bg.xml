<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape>
            <gradient android:angle="90" android:endColor="#ffff4e1d" android:startColor="#ffd8000d"/>
        </shape>
    </item>
    
    <item android:state_pressed="true">
        <shape>
            <gradient android:angle="90" android:endColor="#ffd8000d" android:startColor="#ffff4e1d"/>
        </shape>
    </item>

</selector>